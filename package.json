{
  "name": "HydrogenRnConsumerApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:clean": "cd android && ./gradlew clean",
    "android:prebuild": "rm -rf android/build && rm -rf android/app/build",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "start:clean": "watchman watch-del-all && react-native start --reset-cache",
    "init:template": "yarn install && react-native init $PROJECT_NAME && mv ./$PROJECT_NAME/android ./ && mv ./$PROJECT_NAME/ios ./ && rm -rf ./$PROJECT_NAME && node ./init_template_scripts/init_template_scripts && cd ios && pod install",
    "test": "jest",
    "lint": "eslint 'src/**/*' --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint 'src/**/*' --quiet --fix",
    "e2e:prepare": "node e2e/e2ePrepare.js",
    "e2e:reset": "node e2e/e2eReset.js",
    "e2e:build:ios": "detox build -c ios:release",
    "e2e:test:ios": "detox test -c ios:release -o e2e/config.json -u",
    "e2e:ios": "yarn run e2e:prepare && yarn run e2e:build:ios && yarn run e2e:test:ios && yarn run e2e:reset"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/datetimepicker": "^3.0.2",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/netinfo": "^5.9.7",
    "@react-navigation/bottom-tabs": "^5.6.1",
    "@react-navigation/native": "^5.1.7",
    "@react-navigation/stack": "^5.2.14",
    "@sentry/react-native": "^1.8.2",
    "@stryberventures/stryber-react-native-ui-components": "^0.0.61",
    "@types/lodash": "^4.14.150",
    "axios": "^0.21.1",
    "formik": "^2.2.9",
    "immer": "^9.0.5",
    "lodash": "^4.17.15",
    "moment": "^2.27.0",
    "react": "17.0.2",
    "react-native": "0.64.1",
    "react-native-date-picker": "^3.2.3",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-google-places-autocomplete": "^2.4.1",
    "react-native-input-scroll-view": "^1.10.1",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-maps": "0.28.0",
    "react-native-modal": "^11.5.6",
    "react-native-reanimated": "^1.8.0",
    "react-native-safe-area-context": "^0.7.3",
    "react-native-screens": "^2.7.0",
    "react-native-sensitive-info": "^5.5.8",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-splash-screen": "^3.2.0",
    "react-native-svg": "^12.1.0",
    "react-native-webview": "^11.13.0",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "redux-saga-testing": "^2.0.1",
    "rn-fetch-blob": "^0.12.0",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "@types/jest": "^24.0.24",
    "@types/node": "^13.13.4",
    "@types/react-native": "^0.64.1",
    "@types/react-native-dotenv": "^0.2.0",
    "@types/react-native-snap-carousel": "^3.8.4",
    "@types/react-redux": "^7.1.7",
    "@types/react-test-renderer": "16.9.2",
    "@types/yup": "^0.29.1",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-jest": "^25.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "detox": "^18.20.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "eslint-plugin-react-redux": "^3.0.3",
    "eslint-plugin-redux-saga": "^1.1.3",
    "jest": "^27.0.6",
    "jest-circus": "^27.0.6",
    "metro-react-native-babel-preset": "^0.59.0",
    "react-native-dotenv": "^3.2.0",
    "react-test-renderer": "16.13.1",
    "redux-devtools-extension": "^2.13.8",
    "typescript": "^3.8.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
